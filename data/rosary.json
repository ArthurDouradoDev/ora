  "extraPrayers": {
    "sinal-cruz": {
      "pt": "Em nome do Pai, e do Filho, e do Espírito Santo. Amém.",
      "la": "In nomine Patris, et Filii, et Spiritus Sancti. Amen."
    },
    "fatima": {
      "pt": "Ó meu Jesus, perdoai-nos, livrai-nos do fogo do inferno, levai as almas todas para o Céu, e socorrei principalmente as que mais precisarem.",
      "la": "O mi Iesu, dimitte nobis debita nostra, salva nos ab igne inferiori, perduc in caelum omnes animas, praesertim eas, quae misericordiae tuae maxime indigent."
    },
    "pai-eterno": {
      "pt": "Eterno Pai, eu Vos ofereço o Corpo e Sangue, Alma e Divindade de Vosso diletíssimo Filho, Nosso Senhor Jesus Cristo, em expiação dos nossos pecados e dos do mundo inteiro.",
      "la": "Pater Aeterne, offero tibi Corpus et Sanguinem, Animam et Divinitatem dilectissimi Filii Tui, Domini nostri Iesu Christi, in expiationem peccatorum nostrorum et totius mundi."
    },
    "dolorosa-paixao": {
      "pt": "Pela Sua dolorosa Paixão, tende misericórdia de nós e do mundo inteiro.",
      "la": "Pro dolorosa Eius Passione, miserere nobis et totius mundi."
    },
    "santo-deus": {
      "pt": "Deus Santo, Deus Forte, Deus Imortal, tende piedade de nós e do mundo inteiro.",
      "la": "Sancte Deus, Sancte Fortis, Sancte Immortalis, miserere nobis et totius mundi."
    }
  }
